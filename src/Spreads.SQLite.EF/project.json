{
	"version": "1.7.1-*",
	"description": "SQLite database provider for Entity Framework Core.",
	"packOptions": {
		"tags": [
			"Entity Framework Core",
			"entity-framework-core",
			"EF",
			"Data",
			"O/RM",
			"SQLite"
		]
	},
	"buildOptions": {
		"warningsAsErrors": true,
		"keyFile": "../../tools/Key.snk",
		"nowarn": [
			"CS1591"
		],
		"xmlDoc": true,
		"compile": {
			"include": "../Shared/*.cs"
		},
		"embed": {
			"includeFiles": "Properties/Microsoft.EntityFrameworkCore.Sqlite.rd.xml"
		}
	},
	"dependencies": {
		"Spreads.SQLite": "1.7.1-*",
		"Microsoft.EntityFrameworkCore.Relational": "1.1.0",
		"NETStandard.Library": "1.6.1"
	},
	"keyFile": "../../tools/SpreadsKey.snk",
	"frameworks": {
		"net451": {},
		"netstandard1.6": {}
	}
}